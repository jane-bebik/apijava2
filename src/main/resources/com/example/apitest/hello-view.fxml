<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.apitest.CatSearchController" spacing="10" alignment="CENTER">
  <padding>
    <Insets top="20.0" right="20.0" bottom="20.0" left="20.0"/>
  </padding>
  <HBox spacing="10" alignment="CENTER">
    <TextField fx:id="breedInput" promptText="Enter cat breed"/>
    <ComboBox fx:id="suggestionsDropdown" promptText="Suggestions"/>
  </HBox>
  <Button fx:id="searchButton" text="Search"/>
  <VBox fx:id="resultBox" alignment="CENTER" spacing="10" fillWidth="true"/>
  <Button fx:id="resetButton" text="Reset" visible="false"/>
</VBox>
